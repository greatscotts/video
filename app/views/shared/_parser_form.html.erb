<%= form_tag '/parser' do %>
  <!--%= render 'shared/error_messages', :object => f.object %-->
  <div class="field">
    <%= text_area_tag 'command', params[:command],:maxlength=> 140, :placeholder=> "Holla, holla, playa! Wazzup?" %>
  </div>
  <span class="countdown"></span>
  <%= submit_tag "Post", :class=> "btn btn-large btn-primary", :name=> nil %>
<% end %>
